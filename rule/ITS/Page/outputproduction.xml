<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="outputproduction" mode="on" history="on">
	<description></description>
	<caching>NO_CACHING</caching>
	<xsltfile>pages/page.xsl</xsltfile>
	<element mode="ON" type="INCLUDED_PAGE">
		<name>outline</name>
		<value>outline</value>
	</element>
	<element mode="ON" type="SCRIPT">
		<name>action_bar</name>
		<events>
			<doscript source="file">page.its_actionbar.outputproduction</doscript>
		</events>
	</element>
	<element mode="ON" type="QUERY">
		<name>view_content</name>
		<query source="static">form='outputproduction'</query>
		<sortby source="static">'$sortfield'#'$order'</sortby>
		<doctype>main</doctype>
	</element>
	<caption name="category" source="KEYWORD">Производство</caption>
	<caption name="viewnamecaption" source="KEYWORD">Выпуск продукции</caption>
	<caption name="viewtext1" source="KEYWORD">Название</caption>
	<caption name="viewtext2" source="KEYWORD">Дата</caption>
	<caption name="viewtext3" source="KEYWORD">Количество</caption>
	<caption name="page" source="KEYWORD">Страница</caption>
	<caption name="documents" source="KEYWORD">Документов</caption>
	<caption name="updates" source="KEYWORD">Обновлено</caption>
	<caption name="from" source="KEYWORD">из</caption>
	<caption name="btnNewDocument" source="KEYWORD">Добавить</caption>
	<caption name="btnDelete" source="KEYWORD">Удалить</caption>
	<caption name="helpcaption" source="KEYWORD">Помощь</caption>
	<caption name="logout" source="KEYWORD">Завершить работу</caption>
</rule>
