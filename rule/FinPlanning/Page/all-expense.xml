<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="all-expense" mode="on" history="on">
	<description></description>
	<caching>NO_CACHING</caching>
	<xsltfile>pages/plans.xsl</xsltfile>
	<element mode="ON" type="INCLUDED_PAGE">
		<name>outline</name>
		<value>outline</value>
	</element>
	<element mode="ON" type="SCRIPT">
		<name>saldo</name>
		<events>
			<doscript source="file">finplanning.page.saldo.Calculate</doscript>
		</events>
	</element>
	<element mode="ON" type="INCLUDED_PAGE">
		<name>filter</name>
		<value>filter_type_operation</value>
	</element>
	<element mode="ON" type="SCRIPT">
		<name>action_bar</name>
		<events>
			<doscript source="file">finplanning.page.actionbar.PlanActions</doscript>
		</events>
	</element>
	<element mode="ON" type="SCRIPT">
		<name>view_content</name>
		<events>
			<doscript source="file">finplanning.page.views.AllExpense</doscript>
		</events>
	</element>
	<caption name="viewnamecaption" source="KEYWORD">All expense</caption>
	<caption name="viewtext" source="KEYWORD">Description</caption>
	<caption name="viewnumber" source="KEYWORD">Sum</caption>
	<caption name="viewtext1">Category</caption>
	<caption name="viewtext2">Category ID</caption>
	<caption name="viewtext3">Type of the operation</caption>
	<caption name="viewtext4">Place of the event</caption>
	<caption name="viewtext5">Periodicity</caption>
	<caption name="viewtext6">Receiver or sender</caption>
	<caption name="viewdate" source="KEYWORD">Date</caption>
</rule>
