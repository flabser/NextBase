<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="controloff" mode="on">
	<description>Script to reset control of demand</description>
	<rununderuser source="static">[supervisor]</rununderuser>
	<caching>NO_CACHING</caching>
	<xsltfile></xsltfile>
	<element mode="ON" type="SCRIPT">
		<name>script_result</name>
		<events>
			<doscript source="file">projects.page.controloff.DoScript</doscript>
		</events>
	</element>
	
</rule>
