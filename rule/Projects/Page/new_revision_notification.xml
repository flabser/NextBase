<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="new_revision_notification" mode="on">
	<description>Уведомление о реализации новой доработки</description>
	<rununderuser source="static">[supervisor]</rununderuser>
	<caching>NO_CACHING</caching>
	<xsltfile></xsltfile>
	<element mode="ON" type="SCRIPT">
		<name>script_result</name>
		<events>
			<doscript source="file">page.newRevisionNotification.DoScript</doscript>
		</events>
	</element>
</rule>
