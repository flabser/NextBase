<?xml version="1.0" encoding="UTF-8"?><rule app="" id="mark_current_milestone" isvalid="true" mode="ON" type="HANDLER">
    <description/>
    <rununderuser source="STATIC" type="TEXT">[observer]</rununderuser>
    <trigger>SCHEDULER</trigger>
    <scheduler mode="ON">
        <starttime>15:02</starttime>
        <nextstart/>
        <days>MONDAY,TUESDAY,WEDNESDAY,THURSDAY,FRIDAY</days>
    </scheduler>
    <events>
        <trigger source="file">handler.mark_current_milestone.Trigger</trigger>
    </events>
</rule>