<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="demands" mode="on" history="on">
	<description></description>
	<caching>NO_CACHING</caching>
	<xsltfile>pages/demand.xsl</xsltfile>
	<element mode="ON" type="INCLUDED_PAGE">
		<name>outline</name>
		<value>outline</value>
	</element>
	
	<element mode="ON" type="SCRIPT">
		<name>view_content</name>
		<events>
			<doscript source="file">page.mysupport_demands.DoScript</doscript>
		</events>
	</element>	 
	<caption name="viewdate" source="KEYWORD">Срок исполнения</caption>
	<caption name="viewtext" source="KEYWORD">Краткое содержание</caption>
	<caption name="viewtext2" source="KEYWORD">Проект</caption>	
	<caption name="author" source="KEYWORD">Автор</caption>
	<caption name ="help" source ="KEYWORD">Помощь</caption>
	<caption name ="logout" source ="KEYWORD">Завершить работу</caption>
	<caption name ="documents" source ="KEYWORD">Записей</caption> 	
	<caption name ="search" source ="KEYWORD">Поиск</caption>
	<caption name ="view_userprofile" source ="KEYWORD">Посмотреть свойства текущего пользователя</caption>
	
</rule>