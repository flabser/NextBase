<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="outline" mode="on">
	<description></description>
	<caching>NO_CACHING</caching>
	<element mode="ON" type="SCRIPT">
		<name>current_outline_entry</name>
		<events>
			<doscript source="file">bankfinance.page.nav.CurrentOutlineEntry</doscript>
		</events>
	</element>
	<element mode="ON" type="SCRIPT">
		<name>included_share_navi</name>
		<events>
			<doscript source="file">bankfinance.page.nav.ShareNavigator</doscript>
		</events>
	</element>
	<element mode="ON" type="SCRIPT">
		<name>ws</name>
		<events>
			<doscript source="file">bankfinance.page.ws.AvailableApps</doscript>
		</events>
	</element>
	<caption name="page" source="KEYWORD">Страница</caption>
	<caption name="documents" source="KEYWORD">Документов</caption>
	<caption name="updates" source="KEYWORD">Обновлено</caption>
	<caption name="from" source="KEYWORD">из</caption>
	<caption name="search" source="KEYWORD">Поиск</caption>
	<caption name="help" source="KEYWORD">Помощь</caption>
	<caption name="exit" source="KEYWORD">Выйти</caption>
	<caption name="logout" source="KEYWORD">Завершить работу</caption>
</rule>
